<div class="row">
  <% if (store.beacons) { %>
    <% for (var i=0;i<store.beacons.length;i++) { %>
    <div class="col-lg-3 col-md-6">
      <div class='beacon text-center'>
        <a href="/clients/<%= store.client %>/stores/<%= store._id%>/beacons/<%= store.beacons[i]._id%>">
          <div class='beacon-title bg-light-gray'>
            <div><%= store.beacons[i].beacon_name %></div>
            <div>Minor ID: <%= store.beacons[i].minor_id %></div>
          </div>
          <div class='beacon-body'>
            <div>X: <%= store.beacons[i].position.x %></div>
            <div>Y: <%= store.beacons[i].position.y %></div>
            <div>Z: <%= store.beacons[i].position.z %></div>
          </div>
        </a>
      </div>
    </div>
    <% } %>
  <% } %>
</div>
