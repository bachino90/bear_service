<form id="" class="form-horizontal" action="" method="POST">
  <input type="hidden" value='true' id='test'>
  <div class='form-group'>
    <label class="col-lg-2 control-label">URL (api/v1/)</label>
    <div class="col-lg-10">
      <input type="text" class="form-control" placeholder="" value='malba' id='url'>
    </div>
  </div>
  <div class='form-group'>
    <label class="col-sm-2 control-label">Device OS</label>
    <div class="col-sm-10">
      <input type="text" class="form-control" placeholder="" value='IOS' id='device_os'>
    </div>
  </div>
  <div class='form-group'>
    <label class="col-sm-2 control-label">Device UUID</label>
    <div class="col-sm-10">
      <input type="text" class="form-control" placeholder="" value='12312312-3123-1231-2313-123123123123' id='device_uuid'>
    </div>
  </div>
  <div class='form-group'>
    <label class="col-sm-2 control-label">Beacons</label>
    <div class='col-lg-10' id="beacons">
      <div>
      <div class='col-lg-8'>
        <input type='text' class="form-control" placeholder='FULL UUID' value='11111111-1111-1111-1111-111111111111-1-1' id='full_uuid'>
      </div>
      <div class='col-lg-2'>
        <input type='text' class="form-control" placeholder='RSSI' value='-75' id='rssi'>
      </div>
      <div class='col-lg-2'>
        <input type='button' class="btn btn-danger" value='Delete' id='deleteBeaconButton'>
      </div>
      </div>
    </div>
  </div>
  <div class='form-group'>
    <div class="col-lg-10">
    </div>
    <div class="col-lg-2">
      <input type="button" class="btn btn-default" value="Add Beacon" id='addBeaconButton'>
    </div>
  </div>
  <div class='form-group'>
    <div class="col-lg-10">
    </div>
    <div class="col-lg-2">
      <input type="button" class="btn btn-primary" value="Send" id='sendButton'>
    </div>
  </div>
</form>
